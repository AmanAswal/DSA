class Hash_Table:
    def __init__(self):
        self.Max = 100
        self.arr = [None for i in range(self.Max)]

    def hash_talbe(self, key):
        h = 0
        for char in key:
            h += ord(char)
        return h % 100

    def add(self, key, val):
        h = self.hash_talbe(key)
        self.arr[h] = val

    def get_value(self, key):
        h = self.hash_talbe(key)
        return self.arr[h]


tab = Hash_Table()
